{
    "name":"sirmadman/modler",
    "type":"library",
    "description":"A basic model library forked from enygma/modler",
    "keywords":["model"],
    "homepage":"https://github.com/sirmadman/modler",
    "license":"MIT",
    "authors":[
        {
            "name":"Chris Cornutt",
            "email":"ccornutt@phpdeveloper.org",
            "homepage":"http://www.phpdeveloper.org/"
        },
        {
            "name":"Thomas HÃ¤gglund",
            "email":"thomas.hgglund@gmail.com",
            "homepage":"https://github.com/sirmadman"
        }
    ],
    "support": {
        "source": "https://github.com/enygma/modler"
    },
    "require": {
        "php": ">=8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "squizlabs/php_codesniffer": "3.*"
    },
    "autoload": {
        "psr-4": {
            "Modler\\": "src/Modler"
        },
        "psr-0": {
            "Modler": "src"
        }
    },
    "autoload-dev": {
      "psr-4": {
          "Modler\\Tests\\": "tests/Modler"
      },
      "psr-0": {
          "Modler\\Tests": "tests"
      }
    },
    "scripts": {
        "test": "phpunit tests",
        "phpcs": [
            "phpcs --standard=PSR12 ./src"
        ],
        "phpcbf": [
            "phpcbf --standard=PSR12 ./src"
        ]
    }
}
